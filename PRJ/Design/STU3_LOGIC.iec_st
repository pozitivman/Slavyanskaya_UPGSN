                <Variable UUID="9E81B987458CCD3DEFBB0FAD0696C719" Name="cmd_OFF" Type="R_TRIG" TypeUUID="D90E3B974D4D8C72877123B1A897EB52" Usage="internal" />
                <Variable UUID="57F552494731483CEAC41394B178B526" Name="alg_ON" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="A073A32C4CA560DCBAB5A891FEF7A3F7" Name="alg_OFF" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
            </Variables>
            <ST><![CDATA[KR.Control.RA.0 := HMI_KR_DA;
IF HMI_KR_rOn AND HMI_KR_DA  THEN alg_ON  := BOOL#1; END_IF; HMI_KR_rOn  := BOOL#0;
IF HMI_KR_rOff AND HMI_KR_DA THEN alg_OFF := BOOL#1; END_IF; HMI_KR_rOff := BOOL#0;
IF alg_ON AND CMD_KR_rOnOff OR alg_OFF AND NOT CMD_KR_rOnOff THEN
	KR_rep := BOOL#1;
ELSE
	IF alg_ON THEN CMD_KR_rOnOff := BOOL#1; END_IF;
	IF alg_OFF THEN CMD_KR_rOnOff := BOOL#0; END_IF;
END_IF;
alg_ON := BOOL#0;
alg_OFF := BOOL#0;

KR.Control.rOnOff.0 := CMD_KR_rOnOff;
KR.Control.aOnOff.0 := CMD_KR_aOnOff;
KR.Control.rep.0 := KR_rep; KR_rep := BOOL#0;
KR.Control.cwOn.0 := CMD_KR_cwOn;
KR.Control.cwOff.0 := CMD_KR_cwOff;


KR.Ins.on.0 := DI_KR_ON;
KR.Ins.off.0 := DI_KR_OFF;
KR.Ins.con.0 := DI_KRkcu_ON;
KR.Ins.coff.0 := DI_KRkcu_OFF;
APS_non := KR.Oper.non.0;
APS_noff := KR.Oper.noff.0;
APS_nstart := KR.Oper.nstart.0;
APS_selfon := KR.Oper.selfon.0;
APS_selfoff := KR.Oper.selfoff.0;
APS_flt := KR.Oper.flt.0;
APS_ncon := KR.Oper.ncon.0;
APS_ncoff := KR.Oper.ncoff.0;
KR_state.0 := KR.Ins.off.0;
KR_state.1 := KR.Ins.on.0;


//!!!!!!!!!!!Обобщенный ALARM !!!!!!!!!!!!!!!!!!!
if IMIT then APS_all := false; 
else
	APS_all := APS_non OR APS_noff OR APS_nstart OR APS_selfon OR APS_selfoff OR APS_flt OR APS_ncon OR APS_ncoff OR APS_extern;
end_if;
/////////////////////////////////////////////////


IF NOT HMI_KR_DA THEN CMD_KR_rOnOff := CMD_KR_aOnOff; ELSE CMD_KR_aOnOff := CMD_KR_rOnOff; END_IF;]]></ST>
