                <Variable UUID="F6836D0B435BF4AA6DB953B08C1CF4C5" Name="AE_ON" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="6F8CAF604BEBD8DB8F3FA5AE763411E2" Name="Sound_on" Type="R_TRIG" TypeUUID="D90E3B974D4D8C72877123B1A897EB52" Usage="internal" />
                <Variable UUID="872BBEC941972672CF1E83BEF9611F08" Name="Sound_bool" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
            </Variables>
            <ST><![CDATA[Timer(IN_bool, IN_time, AE_ON, CV_time);
Trigger(AE_ON, Dblck, AE_OUT); //вариант взвода и квитирования АПС Вега-газ

