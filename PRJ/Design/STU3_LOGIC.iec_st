                <Variable UUID="B87FB4864096B15E9846F39C9F228C6D" Name="AO_min" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
                <Variable UUID="978FC92F42B79417F1E8AAB166806DDC" Name="AO_max" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
