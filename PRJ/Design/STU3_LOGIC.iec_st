        <FunctionBlock UUID="2B81CBA042A176E3704178AD77DC9654" Name="FB_AOb" Comment="Аварийный останов без стравливания">
            <Variables />
            <ST><![CDATA[if (Step.AOb <> 2 or Step.AOb <> 3) then TMR[0] := TMR[0] + cycleTime; end_if;
if (TMR[0] >= lreal#300.0) then 
	Step.AOb := 3;
end_if; 
 

case Step.AOb of
	0: //Начало алгоритма
		Step.AOb := 1;

	1:
		AOB_UPGSN := true;
		KC1_STU3_DO_HMI.A3_6_14_SOUND_ON.PV := true; //Включить звуковой сигнал;
		if KC1_STU3_DI_HMI.A2_7_9_V_IN_ON.PV then ///Если открыт входной кран, то закрываем его 
			KC1_STU3_VLV_LG.KR_IN.RA		:= false;
			KC1_STU3_VLV_LG.KR_IN.aOnOff	:= false; 
		end_if;
		if (KC1_STU3_DI_HMI.A2_7_10_V_IN_OFF.PV) then //Входной кран закрыт
		Step.AOb := 2;
        end_if;

	2: //АОб завершен
		AOB_UPGSN_DONE := true;
		AOB_UPGSN := false;
		TMR[0] := lreal#0.0;

	3: //АОб завершен некорректно по превышению времени алгоритма 
		AOB_UPGSN_DONE_INCORRECT := true;
		AOB_UPGSN := false;
		TMR[0] := lreal#0.0;	
end_case;]]></ST>
        </FunctionBlock>
        <FunctionBlock UUID="93FE3F414C253931451739880AEDA7FA" Name="FB_AOs" Comment="Аварийный останов со стравливанием">
            <Variables />
            <ST><![CDATA[if (Step.AOs <> 3 or Step.AOb <> 4) then TMR[1] := TMR[1] + cycleTime; end_if;
if (TMR[1] >= lreal#300.0) then 
	Step.AOs := 4;
end_if; 

case Step.AOs of
	0: //Начало алгоритма
		Step.AOs := 1;

	1:
		AOS_UPGSN := true;
		KC1_STU3_DO_HMI.A3_6_14_SOUND_ON.PV := true; //Включить звуковой сигнал;
		if KC1_STU3_DI_HMI.A2_7_9_V_IN_ON.PV then ///Если открыт входной кран, то закрываем его 
			KC1_STU3_VLV_LG.KR_IN.RA		:= false;
			KC1_STU3_VLV_LG.KR_IN.aOnOff	:= false; 
		end_if;
		if (KC1_STU3_DI_HMI.A2_7_10_V_IN_OFF.PV) then //Входной кран закрыт 
		Step.AOs := 2;
        end_if;

	2: 
		KC1_STU3_DO_HMI.A3_6_6_EO_UPGSN.PV := true; //Взводим сигнал ЭО УПГСН
		KC1_STU3_VLV_LG.KR_CDL.RA		:= false; // Открываем свечной кран
		KC1_STU3_VLV_LG.KR_CDL.aOnOff	:= true; 
		if (KC1_STU3_DI_HMI.A2_7_11_V_CDL_ON.PV) then //Входной кран закрыт
		Step.AOs := 3;
        end_if;

	3: //АОс завершен
		AOS_UPGSN_DONE := true;
		AOS_UPGSN := false;
		TMR[1] := lreal#0.0;

	4: //АОc завершен некорректно по превышению времени алгоритма 
		AOS_UPGSN_DONE_INCORRECT := true;
		AOS_UPGSN := false;
		TMR[1] := lreal#0.0;
end_case;]]></ST>
        </FunctionBlock>
