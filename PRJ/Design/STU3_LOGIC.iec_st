            <Function UUID="306BC47D4D7CBF0A6CBC8DB34BCB6CA0" Name="MAX_VG" Comment="Выбор максимального из двух REAL" ResultTypeUUID="42E054C8453789BBDD3594B53BDC7DE5">
                <Variables>
                    <Variable UUID="318EE2B4466D19541BA42CBF55EE074E" Name="IN1" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="E66F8AA148E6029657733E9AEE4D6D0B" Name="IN2" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                </Variables>
                <ST><![CDATA[// ===============================================================================
// функция, возвращающая максимальное из двух значений real на входе
// ===============================================================================
MAX_VG := IN1;
IF IN2 > IN1 THEN MAX_VG := IN2; END_IF;]]></ST>
            </Function>
            <Function UUID="6347D59B429F99B5F49133BAFCA594FD" Name="MIN_VG" Comment="Выбор минимального из двух REAL" ResultTypeUUID="42E054C8453789BBDD3594B53BDC7DE5">
                <Variables>
                    <Variable UUID="F5AD924D421492278109308890C6FDE6" Name="IN1" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="E893E0CE45BC3FFCDC19C59875FA8B38" Name="IN2" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                </Variables>
                <ST><![CDATA[// ===============================================================================
// функция, возвращающая минимальное из двух значений real на входе
// ===============================================================================
MIN_VG := IN1;
IF IN2 < IN1 THEN MIN_VG := IN2; END_IF;]]></ST>
            </Function>
            <FunctionBlock UUID="FC714E38406E84024B957D9AFF39A636" Name="SinglePID_Proc">
                <Variables>
                    <Variable UUID="A821E1A34809244B03EE6CBE74B3E4C3" Name="init" Comment="Признак инициализации" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    <Variable UUID="39698A3F49015EA51A56F6894D3331A9" Name="errTemp" Comment="Ошибка за вычетом мертвой зоны" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
                    <Variable UUID="5313E97F485A328663F60A9EF1409269" Name="outTemp" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
                    <Variable UUID="EED7A9DC4ABF19AF19B5CFBE8B9E9CB2" Name="dInteg" Comment="Приращение интегратора" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
                    <Variable UUID="84C4DDD74CCA1E59B865CE8E10F25A70" Name="integ" Comment="Интегратор" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
                    <Variable UUID="CEBEB7D349B328D0FB4E29A5C59E0618" Name="oldErr" Comment="Ошибка регулирования на предыдущем цикле" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
                    <Variable UUID="945BAEE341161C95A2BAE290C252A466" Name="tmp" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
                    <Variable UUID="017812B04DE5DE29B98E4093D83CD4E2" Name="err" Comment="Ошибка регулирования" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="35D48CBE45DEDF4CA08E36AAE5B6A0B8" Name="hiLimit" Comment="Верхнее ограничение выхода регулятора" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="47E60A394E7D178306FB5F975AB9BB5C" Name="loLimit" Comment="Нижнее ограничение выхода регулятора" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="CB88F1FD41587A06C944F4AC8E358D4F" Name="hiRate" Comment="Ограничение по скорости увеличения выхода регулятора" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="6C4B0800437526861AD6989271A3CE20" Name="loRate" Comment="Ограничение по скорости уменьшения выхода регулятора" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="CB7A074545E7342B1F49BD9DCC7D51EC" Name="cycle" Comment="Время цикла вызова регулятора" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="E2FFC27746F55AE887DE8DAD08D50F3D" Name="setDU" Comment="Выход регулятора при ДУ" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="CA2F4A8F4AFCA8B3E9537EA01341992E" Name="DU" Comment="Признак ДУ" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                    <Variable UUID="9BD7B2B64F5CE2F26652759D388B963C" Name="Work_ON" Comment="Включить регулятор" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                    <Variable UUID="D78D4506404D0EF322D6EA95D3C84485" Name="Kp" Comment="Пропорциональный коэффициент" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="9FEFB7974EA3FF9CDAE9EA9C83789368" Name="Ki" Comment="Интегральный коэффициент" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="60371F03451EB444BC01A3A066F82C0E" Name="Kd" Comment="Дифференциальный коэффициент" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="E4A0DC8C4BA647EF577BD2A9C2DDEC2E" Name="DZ" Comment="Зона нечувствительности" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                    <Variable UUID="8DBB83C243E95D50BCAC14A08A33FE4D" Name="out" Comment="Выход регулятора" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="output" />
                </Variables>
                <ST><![CDATA[//Инициализация
init := Work_ON;
	IF NOT init THEN
    	dInteg  := 0.0;
    	oldErr  := err;
    	out     := setDU;
		integ   := setDU;
   		init    := TRUE;
	ELSE
  //Если осуществляется нормальная работа
	IF NOT DU THEN  
    //Расчет ошибки, пропущенной через зону нечувствительности
		IF err > DZ THEN errTemp := err - DZ; ELSIF err < -DZ THEN errTemp := err + DZ; ELSE errTemp := 0.0; END_IF;
    //Расчет приращения интегратора
		dInteg := (errTemp + oldErr) / 2.0 * cycle * Ki;
    //Расчет выхода регулятора
		outTemp := integ + dInteg + ((errTemp - oldErr)/cycle) * Kd + errTemp * Kp;
		oldErr := errTemp;
  //Если осуществляется дистанционное управление
	ELSE
    //Приращение интегратора расчитывается как разность задания при ДУ и текущего выхода регулятора
		dInteg  := setDU - out;
    //Ошибка регулирования считается равной нулю
		oldErr  := 0.0;
    //Выход приравнивается заданию при ДУ
		outTemp := setDU;
	END_IF;
  //Пересчет выхода и приращения интегратора с учетом ограничения скорости изменения выхода
	IF (outTemp - out) > hiRate * cycle  THEN 
		out := out + hiRate * cycle;
		dInteg := MIN_VG(hiRate * cycle, dInteg);
	ELSIF (outTemp - out) < -loRate * cycle THEN
		out := out - loRate * cycle;     
		dInteg := MAX_VG(-loRate * cycle, dInteg);
	ELSE
		out := outTemp;
	END_IF;
	//Расчет приращения интегратора
		integ := MIN_VG(MAX_VG(integ + dInteg, loLimit), hiLimit);
	//Учет ограничения выхода
		out := MIN_VG(MAX_VG(out,loLimit), hiLimit);
	END_IF;
]]></ST>
            </FunctionBlock>
        </Folder>
        <FunctionBlock UUID="6A4FEBAC4B635BACDB1140A38C63D50E" Name="AE_Proc">
