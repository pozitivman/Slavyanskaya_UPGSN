        <Function UUID="EF7E27804569F442851A6D97DD18BBCE" Name="AI_Scale_func" ResultTypeUUID="42E054C8453789BBDD3594B53BDC7DE5">
            <Variables>
                <Variable UUID="A9EE377B407523DE221B85A885DDD943" Name="In" Type="UINT" TypeUUID="C50B45D04CDCD1FA8859FAB8CB6FC7EE" Usage="input" />
                <Variable UUID="19C166A840F8D0BB4A431DA2DE87C093" Name="K1" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="76D8A0714EEBF3E4DDF38F981000702F" Name="K2" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="82194E4F4FBAFCAAD5DA31981D813997" Name="LO_LIM" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="3BD923F0483F0F94C1B65781DA23E915" Name="HI_LIM" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
            </Variables>
            <ST><![CDATA[AI_Scale_func := (((uint_to_real(In) - K1) / (K2 - K1)) * (HI_LIM - LO_LIM)) + LO_LIM;]]></ST>
        </Function>
        <FunctionBlock UUID="F29B0A244FCF53C51057D8A2000C32B0" Name="AI_TO_DI" Comment="Обработка аналога в дискрет">
            <Variables>
                <Variable UUID="5E2516F441C47CF487ACE48564237B73" Name="HI_LO" Comment="Тип датчика (HLFS или LLFS)" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="092E8B1142AA1D9513A45DAF7B31EA53" Name="AI_IN" Comment="Измеряемый ток датчика" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="69C101D84537FED61739ABB235826BD8" Name="DB" Comment="Зона нечувствительности" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="97C292264DEF626C2E785F89036270C0" Name="OUT_DI" Comment="Срабатывание датчика" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="E8D3866C49DDFE30F47E2F9FE1C7CDFD" Name="DI_FLT" Comment="Ошибка датчика" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="3B26939B4EFFBD1CC43C4CB436ABAB21" Name="rsDI" Type="RS" TypeUUID="53F400294BCD7C71EB041B90164F4F6F" Usage="internal" />
                <Variable UUID="0D316DE644F0E427BAB8FB8DB5DD2544" Name="LVL_DRY" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
                <Variable UUID="10E5612E44EF70DCB42655AFBEF23A25" Name="LVL_WET" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
            </Variables>
            <ST><![CDATA[//Определяем границы измерений тока по типу датчика
if HI_LO then
	LVL_DRY := real#8.0;
	LVL_WET := real#16.0;
else
	LVL_DRY := real#16.0;
	LVL_WET := real#8.0;
end_if;	


//Определяем ошибку датчика
DI_FLT := (AI_IN <= real#3.6) or (AI_IN >= real#22.0);


if not DI_FLT then
//Срабатывание датчика, установленного в нижней части емкости
	if not HI_LO then
		rsDI(S:= (AI_IN >= LVL_DRY - DB) and (AI_IN <= LVL_DRY + DB), R1:= (AI_IN >= LVL_WET - DB) and (AI_IN <= LVL_WET + DB), Q1 => OUT_DI);
	end_if;
//Срабатывание датчика, установленного в верхней части емкости
	if HI_LO then
		rsDI(S:= (AI_IN >= LVL_WET - DB) and (AI_IN <= LVL_WET + DB), R1:= (AI_IN >= LVL_DRY - DB) and (AI_IN <= LVL_DRY + DB), Q1 => OUT_DI);
	end_if;
end_if;]]></ST>
        </FunctionBlock>
        <Function UUID="8D15F57049F5028192F2088F3E72CEF6" Name="AI_init" ResultTypeUUID="A32E304D43358DC1574ABBA8FDAFBC2D">
            <Variables>
                <Variable UUID="CC94540247523423E3460AB137780F9C" Name="Span" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="58B18438491EDC36229D6D88D269FA99" Name="Offset" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="22E4891641AECA58CF714D89CA6D0C77" Name="Tf" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="00846179436EC25E98CA879644FC0731" Name="repair_time" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="2055DDD5435182E309081FA6A061CECF" Name="min_ADC" Type="DINT" TypeUUID="AF9665534C6D5BDEF9AE77B026938B65" Usage="input" />
                <Variable UUID="91DB535A4BA9A454B6863C988A5C4DD4" Name="max_ADC" Type="DINT" TypeUUID="AF9665534C6D5BDEF9AE77B026938B65" Usage="input" />
                <Variable UUID="A13B68614D59FF3AF0E6C8A761E11F7B" Name="Eu_sensor" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                <Variable UUID="E087522A4909BD7C48B172AAA8B8B9A1" Name="Eu_PV" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                <Variable UUID="87E4772D45FC9CEAD17A5FAD205247E6" Name="Eu_user" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                <Variable UUID="CF2C2E39497E9CD0D29EF8AB802E44AE" Name="Manual_Enable" Type="BYTE" TypeUUID="C3C10A82401AD741FA0AB89DDE8B5F30" Usage="input" />
                <Variable UUID="3D43BBF34F3DA111E5AFB18F1F9A49D4" Name="Manual_Auto" Type="BYTE" TypeUUID="C3C10A82401AD741FA0AB89DDE8B5F30" Usage="input" />
                <Variable UUID="46E9F64E489A651A13F21297DE53702A" Name="Manual_Target" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="9CCFA974419BEA72636DCE9DEC768273" Name="HMI_INIT" Type="AI_HMI" TypeUUID="A66BBC2040DE5F1DB91157BC6ACE32E7" Usage="inout" />
                <Variable UUID="AB367F4B47FD3A1C43CDAF9E6C9AC8EB" Name="ROC" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="439EEEA4444A700DD2AE198A0BDFD689" Name="Descr" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" InitialValue="&apos;FALSE&apos;" Usage="input" />
            </Variables>
            <ST><![CDATA[AI_init.Config.Span:=Span;
AI_init.Config.Offset:=Offset;
AI_init.Config.gain := LREAL#0.0;
AI_init.Config.bias := LREAL#0.0;
AI_init.Config.ROC := ROC;	//Скорость нарастания сигнала
AI_init.Config.Tf:=Tf;
AI_init.Config.Manual_Target := Manual_Target;
AI_init.Config.Manual_Rate := Span/LREAL#20;
AI_init.Config.min_fault_sensor_Eu := Offset-Span*LREAL#0.025;
AI_init.Config.max_fault_sensor_Eu := Span+Offset+Span*LREAL#0.012;
AI_init.Config.recovery_time := LREAL#0.5;
AI_init.Config.repair_time := repair_time;
AI_init.Config.min_ADC := DINT_TO_LREAL(min_ADC);
AI_init.Config.max_ADC := DINT_TO_LREAL(max_ADC);
AI_init.Config.Eu_sensor := Eu_sensor;
AI_init.Config.Eu_PV := Eu_PV;
AI_init.Config.Eu_user := Eu_user;
AI_init.Config.TypePolynom := INT#0;
AI_init.Config.Manual_Enable := Manual_Enable;
AI_init.Config.Manual_Auto := Manual_Auto;
AI_init.Config.derivative_Enable := BYTE#0;
AI_init.Config.ROC_Enable := BYTE#0;
AI_init.Config.reset := BYTE#0;

HMI_INIT.Eu 		:= Eu_user;							//Eu  для SCADA
HMI_INIT.TM_Repair 	:= repair_time;						//Время вывода из ремонта
HMI_INIT.ROC 		:= ROC;								//Ошибка скорости измерения сигнала
HMI_INIT.Max 		:= LREAL_TO_REAL(Offset + Span);	//Максимум диапазона
HMI_INIT.Min 		:= LREAL_TO_REAL(Offset);			//Минимум диапазона
HMI_INIT.Descr		:= Descr;							//Название сигнала (Комментарий)]]></ST>
        </Function>
        <FunctionBlock UUID="152DC66D472DA5BCE4BACEB7ACAF2FBC" Name="AO_Scale">
            <Variables>
                <Variable UUID="1F94AC3B4EA757E1C5D2029034736BB3" Name="In" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="A091D204495A7245B91672BCC5CAD872" Name="Out" Type="UINT" TypeUUID="C50B45D04CDCD1FA8859FAB8CB6FC7EE" Usage="output" />
                <Variable UUID="250A39DB4F5873111658D1880FCD9D5D" Name="K1" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="F373E3DD41D972A5536318A0131891E5" Name="K2" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="CAB4D4594CF7796CB59FCAB719D7606E" Name="LO_LIM" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="CB9CA93F47E5A6BDDE6C6DBA3A901A0F" Name="HI_LIM" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="input" />
                <Variable UUID="287141B242EE55B6B7A5EDA717481DF1" Name="In_int" Type="REAL" TypeUUID="42E054C8453789BBDD3594B53BDC7DE5" Usage="internal" />
            </Variables>
            <ST><![CDATA[//K1 := REAL#11937.0; выдача 4 мА
//K2 := REAL#59550.0; выдача 20 мА
//LO_LIM := LREAL#0.0;
//HI_LIM := LREAL#100.0;
 

if (In < LO_LIM) then 
	Out := real_to_uint(K1);
end_if;

if (In > HI_LIM) then 
	Out := real_to_uint(K2);
end_if;

if (In >= LO_LIM and In <= HI_LIM) then    
	Out := real_to_uint((((In - LO_LIM) / (HI_LIM - LO_LIM)) * (K2 - K1)) + K1);
end_if;]]></ST>
        </FunctionBlock>
        <FunctionBlock UUID="8241A1A94DC7CD2DAF4A08841D436394" Name="Ctrl_crane">
            <Variables>
                <Variable UUID="82DB17654652389B8E9026BFE5E0043F" Name="PRU_DA_on" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="630D382B4428667D7CF1B4B25F67FC0D" Name="PRU_DA_off" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="F0AA46C8429DB17DFDCA94A0F1C228E4" Name="HMI_DA_on" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="59DDC0DE43A4921B4FC5049508EB3F21" Name="HMI_DA_off" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="2B8E09F841AC43C75984BB9F4BBB569F" Name="PRU_rOn" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="62802A5B4E680177727E43AA93133594" Name="PRU_rOff" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="577F618C49FF544C1823F5A50DB15121" Name="HMI_rOn" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="8E2AE4714002E8F2AEAC5283280EE478" Name="HMI_rOff" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="73F1D5D74FC303C22CE3C29F5A8151B9" Name="LG_state" Type="WORD" TypeUUID="05CD1AAB4891B30C004C2799CA460884" Usage="input" />
                <Variable UUID="B4241ADC46CEED6CB48E978AEACCD611" Name="LG_flt_all" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="7BBEEA7D44FD2737525B40A87A86A8AF" Name="PRU_state" Type="WORD" TypeUUID="05CD1AAB4891B30C004C2799CA460884" Usage="inout" />
                <Variable UUID="864DE8364A8C9FE134B595B047138C37" Name="HMI_state" Type="WORD" TypeUUID="05CD1AAB4891B30C004C2799CA460884" Usage="inout" />
                <Variable UUID="BC1E72B44C026D240F32D2818F430749" Name="PRU_flt_all" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="072059D64441AD36402E2C8C25296063" Name="HMI_flt_all" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="2B5188684FACDC33646582A4BE7C8058" Name="DA" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="0240C45844E18D10DA6B64A6E90A10A1" Name="DA_PRU" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="35ABCCB44FA5E76BE05FF884CB8BCB36" Name="DA_HMI" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="77225DF145E844B392BAA0ABA49A8202" Name="rOn" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="CA50BEC9482710C5792FB98ABD3BBD0C" Name="rOff" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
            </Variables>
            <ST><![CDATA[//перевод ИМ в дистанционный/автоматический режим
if PRU_DA_on or HMI_DA_on then DA := BOOL#1; end_if;
if PRU_DA_off or HMI_DA_off then DA := BOOL#0; end_if;

//Передаем руч/дист ИМ из блока в разные приемники
if DA then
	DA_PRU := true;
	DA_HMI := true;
elsif not DA then 
	DA_PRU := false;
	DA_HMI := false;
end_if;

//Передаем обобщенную аварию ИМ из блока в разные приемники
if LG_flt_all then
	PRU_flt_all := true;
	HMI_flt_all := true;
else
	PRU_flt_all := false;
	HMI_flt_all := false;
end_if;

//Передаем состояние ИМ из блока в разные приемники
PRU_state := LG_state;
HMI_state := LG_state;
	
//Дистанционное управление ИМ
if PRU_rOn OR HMI_rOn then rOn := BOOL#1; end_if;
if PRU_rOff OR HMI_rOff then rOff := BOOL#1; end_if;]]></ST>
        </FunctionBlock>
        <FunctionBlock UUID="90D0C5D044939603D0BD418B90A10328" Name="DI_Proc">
            <Variables>
                <Variable UUID="293790C0400AEE8E646241AF54168597" Name="DI_raw" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="90D020944A62F4359AC073BD30D91C04" Name="cycle_set" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                <Variable UUID="6DA04EE34C5683D9CA5E5DA173F1E282" Name="Inv" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="9EBB87F74FDE47608E622599DCEFC25E" Name="DI_out" Type="DI_HMI" TypeUUID="24096E3F4C0C396B81199DA606D417FF" Usage="inout" />
                <Variable UUID="9238B59F4BC1A60E4E950DBC947C2480" Name="Descr" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" InitialValue="&apos;FALSE&apos;" Usage="input" />
                <Variable UUID="74FB70564CC19DC7C703E6B32B39C126" Name="cycle_val" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                <Variable UUID="AFC9DC1340331290F03520A18FE89153" Name="DI_in_last" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
            </Variables>
            <ST><![CDATA[IF NOT (DI_Raw XOR DI_in_last) THEN
	cycle_val := cycle_val + INT#1;
ELSE
	cycle_val := INT#0;
END_IF;

IF cycle_val >= cycle_set THEN
	DI_out.PV := DI_Raw XOR Inv;
END_IF;

DI_in_last := DI_Raw;
DI_out.Descr := Descr;]]></ST>
        </FunctionBlock>
