        <FunctionBlock UUID="3F4E01994D0F833BB767D2A7EC992919" Name="FB_ALG">
            <Variables>
                <Variable UUID="BEA20CA14FFD8F290D0293875E76CB26" Name="rstrigLamp" Type="RS" TypeUUID="53F400294BCD7C71EB041B90164F4F6F" Usage="internal" />
                <Variable UUID="E6F50F874871AF304EAF2D9EC9A0B9BB" Name="proc_AOb" Type="FB_AOb" TypeUUID="2B81CBA042A176E3704178AD77DC9654" Usage="internal" />
                <Variable UUID="4BC867454E0AF18224932F991181FD36" Name="proc_AOs" Type="FB_AOs" TypeUUID="93FE3F414C253931451739880AEDA7FA" Usage="internal" />
                <Variable UUID="04C13F5F4D74E90B8E11ADB32525E4E4" Name="rstrigLampRA" Type="RS" TypeUUID="53F400294BCD7C71EB041B90164F4F6F" Usage="internal" />
                <Variable UUID="6AB1597D4F7DF2036058E48BCD2180A1" Name="rtrigV_TO_ON" Type="R_TRIG" TypeUUID="D90E3B974D4D8C72877123B1A897EB52" Usage="internal" />
                <Variable UUID="3A7A374D4B6515F76ECD028976644FDE" Name="SV_GB_ON_prev" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="9773124F4B1C5ECE19A6C08B2CBA9037" Name="V_TO_ON_prev" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="20363EBD4FED3176651B629A94B678D6" Name="APS_AOB_prev" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="9839F8514081A43CDF4462B44D25EC2A" Name="APS_AOS_prev" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="F997E5E24B6293F83E2B6EA2CC9F6647" Name="Zagaz_CH4_10p_optn_prev_on" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="31B92FA14712D9D03E901DAD253F924E" Name="Zagaz_CH4_10p_optn_prev_off" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
            </Variables>
            <ST><![CDATA[//Взвод и снятие флага Проверка защит
if MD_PZ_PRU_on or MD_PZ_HMI_on then MD_PZ := BOOL#1; end_if;
if MD_PZ_PRU_off or MD_PZ_HMI_off then MD_PZ := BOOL#0; end_if;


//В.2.3 Включение/отключение наружного освещения
time_now	:= CURRENT_DATE_TIME();
hour		:= GET_HOURS(time_now);
min			:= GET_MINUTES(time_now);
sec			:= GET_SECONDS(time_now);
year		:= GET_YEAR(time_now);
month		:= GET_MONTH(time_now);
day			:= GET_DAY(time_now);		

//Автоматическое управление наружным освещением по времени
if hour = ust.lightOn_h and min = ust.lightOn_m then 
	LIGHT_ON := true; 
elsif hour = ust.lightOff_h and min = ust.lightOff_m then 
	LIGHT_ON := false; 
end_if;

if not Lamp.RA then
	KC1_STU3_DO_HMI.A3_6_2_EXT_LIGHT_ON.PV := LIGHT_ON;
end_if;

//Дистанционное управление наружным освещением
rstrigLampRA(S:= Lamp.RA_on, R1:= Lamp.RA_off, Q1=> Lamp.RA);
rstrigLamp(S:= Lamp.rOn and Lamp.RA, R1:= Lamp.rOff and Lamp.RA, Q1=> KC1_STU3_DO_HMI.A3_6_2_EXT_LIGHT_ON.PV);


//В.6 Управление работой котлов
if  KC1_STU3_DI_HMI.A2_6_9_Zagaz_CH4_10p_optn.PV = false and not Zagaz_CH4_10p_optn_prev_on then
	KC1_STU3_VLV_LG.KR_SV1_1.RA			:= false;
	KC1_STU3_VLV_LG.KR_SV1_1.aOnOff		:= true; // Открываем клапан SV1.1

	KC1_STU3_VLV_LG.IM_H_GB1_1.RA		:= false;	
	KC1_STU3_VLV_LG.IM_H_GB1_1.aOnOff	:= true; // Разрешаем работу GB1.1

	KC1_STU3_VLV_LG.IM_H_GB1_2.RA		:= false;	
	KC1_STU3_VLV_LG.IM_H_GB1_2.aOnOff	:= true; // Разрешаем работу GB1.2
end_if;
Zagaz_CH4_10p_optn_prev_on := not KC1_STU3_DI_HMI.A2_6_9_Zagaz_CH4_10p_optn.PV;

if  KC1_STU3_DI_HMI.A2_6_9_Zagaz_CH4_10p_optn.PV = true and not Zagaz_CH4_10p_optn_prev_off  then
	KC1_STU3_VLV_LG.KR_SV1_1.RA			:= false;
	KC1_STU3_VLV_LG.KR_SV1_1.aOnOff		:= false; // Закрываем клапан SV1.1

	KC1_STU3_VLV_LG.IM_H_GB1_1.RA		:= false;	
	KC1_STU3_VLV_LG.IM_H_GB1_1.aOnOff	:= false; // Запрещаем работу GB1.1

	KC1_STU3_VLV_LG.IM_H_GB1_2.RA		:= false;	
	KC1_STU3_VLV_LG.IM_H_GB1_2.aOnOff	:= false; // Запрещаем работу GB1.2
end_if;
Zagaz_CH4_10p_optn_prev_off := not KC1_STU3_DI_HMI.A2_6_9_Zagaz_CH4_10p_optn.PV;

//В.7 Управление вентилятором ТО
//Фиксируем нажатие кнопки включения вентилятора ТО
rtrigV_TO_ON(CLK:= KC1_STU3_DI_HMI.A2_6_3_V_TO_ON.PV, Q => btn_V_TO_ON);
if KC1_STU3_DI_HMI.A2_6_4_Fire1ot.PV then blc_V_TO_on := true; else blc_V_TO_on := false; end_if; // Блокировка работы вентилятора по сигналу Пожар в технологическом отсеке

alg.V_TO_ON := (KC1_STU3_DI_HMI.A2_6_7_Zagaz_CH4_10p_ot.PV or KC1_STU3_DI_HMI.A2_6_8_Zagaz_CH4_20p_ot.PV or btn_V_TO_ON) and not blc_V_TO_on and not V_TO_ON_prev;
if alg.V_TO_ON then
	KC1_STU3_VLV_LG.IM_V_TO.RA		:= false;
	KC1_STU3_VLV_LG.IM_V_TO.aOnOff	:= true; // Включаем вентилятор ТО
end_if; 
V_TO_ON_prev := alg.V_TO_ON; 

if not alg.V_TO_ON and KC1_STU3_VLV_LG.IM_V_TO.RA = false and KC1_STU3_VLV_LG.IM_V_TO.aOnOff = true then
	TMR[2] := TMR[2] + cycleTime;
else TMR[2] := lreal#0.0;
end_if;
if (TMR[2] >= lreal#300.0) or KC1_STU3_DI_HMI.A2_6_4_Fire1ot.PV then 
	KC1_STU3_VLV_LG.IM_V_TO.RA		:= false;
	KC1_STU3_VLV_LG.IM_V_TO.aOnOff	:= false; // Отключаем вентилятор ТО по пропаданию причины или по сигналу Пожар.
end_if; 
  

//В.8 Аварийный останов системы
//Формирование флага АОб
if (Global_PRU.Btn_AOB						or									//Нажатие АОБ на ПРУ
	APS_KC1_STU3.A1_6_3_Pg2_out1_hi			or									//Давление газа на выходе 1 - высокое
	APS_KC1_STU3.A2_7_1_Pig_hi				or									//Давление испульсного газа высокое
	APS_KC1_STU3.A2_6_8_Zagaz_CH4_20p_ot	or									//Загазованность по метану 20% НКПР (технологический отсек)
	APS_KC1_STU3.A2_6_10_Zagaz_CH4_20p_optn or									//Загазованность по метану 20% НКПР (отсек подготовки теплоносителя)
	APS_KC1_STU3.A2_6_12_Zagaz_CO_100mgm3	or									//Загазованность по CO 100 мг/м³
	APS_KC1_STU3.A2_6_13_Fail_PK			or									//Неисправность ПК
	APS_KC1_STU3.A2_6_14_Fail_ASPS_SKZ_SOUE) and not MD_PZ and not APS_AOB_prev	//Неисправность АСПС, СКЗ и СОУЭ
	then 
		UnitState.State := 1;
		allow_bit_stop_AOB := false;
else
	allow_bit_stop_AOB := true;	
end_if;
APS_AOB_prev :=
				Global_PRU.Btn_AOB						or
 				APS_KC1_STU3.A1_6_3_Pg2_out1_hi			or
				APS_KC1_STU3.A2_7_1_Pig_hi				or
				APS_KC1_STU3.A2_6_8_Zagaz_CH4_20p_ot	or
				APS_KC1_STU3.A2_6_10_Zagaz_CH4_20p_optn or
				APS_KC1_STU3.A2_6_12_Zagaz_CO_100mgm3	or
				APS_KC1_STU3.A2_6_13_Fail_PK			or
				APS_KC1_STU3.A2_6_14_Fail_ASPS_SKZ_SOUE;


//Формирование флага АОс
if (Global_PRU.Btn_AOS				or							//Нажатие АОБ на ПРУ
	APS_KC1_STU3.A2_6_4_Fire1ot 	or 							//Пожар в технологическом отсеке
	APS_KC1_STU3.A2_6_5_Fire1otn	or 							//Пожар в отсеке подготовки теплоносителя
	APS_KC1_STU3.A2_6_6_Fire1ou		or 							//Пожар в отсеке управления
	APS_KC1_STU3.A2_7_8_ESD			or 							//Аварийный останов
	APS_KC1_STU3.A2_7_13_EAO) and not MD_PZ and not APS_AOS_prev//Экстренный останов
	then  
		UnitState.State := 2;
		allow_bit_stop_AOS := false;
else
	allow_bit_stop_AOS := true;	
end_if;
APS_AOS_prev :=
				Global_PRU.Btn_AOS				or
				APS_KC1_STU3.A2_6_4_Fire1ot 	or
				APS_KC1_STU3.A2_6_5_Fire1otn	or
				APS_KC1_STU3.A2_6_6_Fire1ou		or
				APS_KC1_STU3.A2_7_8_ESD			or
				APS_KC1_STU3.A2_7_13_EAO;


//При включенном сигнале "Звуковой сигнал" и поступлении сигнала "Квитирование", отключаем сигнал "Звуковой сигнал"
if (KC1_STU3_DO_HMI.A3_6_14_SOUND_ON.PV and Kvitir) then 
	KC1_STU3_DO_HMI.A3_6_14_SOUND_ON.PV := false;
end_if;  				

if (UnitState.State = 0) then
	Step.AOb := 0;
	Step.AOs := 0;
end_if;

case UnitState.State of
	0: //Нормальная работа САУ

	1: //АО без стравливания
		proc_AOb();			

	2: //АО со стравливанием 
		proc_AOs();			
end_case;

//Деблокировка после АОб
if ((UnitState.State = 1 and (Step.AOb = 2 or Step.AOb = 3)) and Reset_AO) or (allow_bit_stop_AOB and Reset_AO) then
	KC1_STU3_DO_HMI.A3_6_14_SOUND_ON.PV := false;
	UnitState.State := 0;
	AOB_UPGSN_DONE := false;
	AOB_UPGSN_DONE_INCORRECT := false;
end_if;  

//Деблокировка после АОс
if ((UnitState.State = 2 and (Step.AOs = 3 or Step.AOs = 4)) and Reset_AO) or (allow_bit_stop_AOS and Reset_AO) then
	KC1_STU3_DO_HMI.A3_6_14_SOUND_ON.PV := false;
	KC1_STU3_DO_HMI.A3_6_6_EO_UPGSN.PV	:= false;
	UnitState.State := 0;
	AOS_UPGSN_DONE := false;
	AOS_UPGSN_DONE_INCORRECT := false;
end_if;


//Промежуточные вычисления
UPIG1.mes.Pimp_gaza_calc := int_to_real(UPIG1.mes.Pimp_gaza)/real#1000.0;
UPIG1.mes.Pproduvki_calc := int_to_real(UPIG1.mes.Pproduvki)/real#1000.0;
UPIG1.mes.dP_UPIG_calc := int_to_real(UPIG1.mes.dP_UPIG)/real#1000.0;
UPIG1.mes.Tads_1_calc := int_to_real(UPIG1.mes.Tads_1)/real#10.0;
UPIG1.mes.Tads_2_calc := int_to_real(UPIG1.mes.Tads_2)/real#10.0;
UPIG1.mes.Tros_calc := int_to_real(UPIG1.mes.Tros)/real#10.0;

]]></ST>
