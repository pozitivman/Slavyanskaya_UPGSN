        <Variable UUID="C0D3978D49ADB407CC36E9B156F085DB" Name="pause_link" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="internal" />
        <Variable UUID="5B6236684AFD43B7693F9C8A189B1C7C" Name="proc_PID_calc" Type="FB_PID_CALC" TypeUUID="9492448C4E812292EA6CB5B1185D6694" Usage="internal" />
    </Variables>
    <ST><![CDATA[//Использование C-библиотеки (время цикла и таймер 1с)
clock_AI_cur := CLOCK();
IF Firstscan THEN
	clock_AI_prev := clock_AI_cur;
END_IF;
cycleTime := clock_AI_cur - clock_AI_prev;
clock_AI_prev := clock_AI_cur;


//Проверка связи с ПРУ
if not Global_PRU.Check_link_bit then
	pause_link := pause_link + cycleTime;
else
	pause_link := lreal#0.0;
end_if;
if pause_link >= lreal#5.0 then
	PRU_Link := word#2;
else
	PRU_Link := word#0;	
end_if;
Global_PRU.Check_link_bit := false;  


//Фиксируем нажатие кнопки Квитирование на щите САУ
rtrig_Kvitir(CLK:= KC1_STU3_DI_HMI.A2_9_5_ACK_SOUND.PV, Q => Kvitir);
//Фиксируем нажатие кнопки Сброс аварийной остановки на щите САУ
rtrig_rst_AO(CLK:= KC1_STU3_DI_HMI.A2_9_6_RST_AO.PV, Q => Reset_AO);
//Фиксируем нажатие кнопки Деблокировка из разных источников
rtrig_Deblock(CLK:= Global_PRU.Deblock or Deblock_HMI, Q => Deblock);

proc_AI();
proc_DI();

proc_ALG();
proc_VLV();

proc_PID_calc(cycle := LREAL_TO_REAL(cycleTime));
proc_DO();
proc_AO();
proc_APS();

if DO_sound_PS then
	KC1_STU3_DO_HMI.A3_7_11_SOUND_ALARM1_ON.PV	:= true; //Включить предупредительную сигнализацию на щите САУ УПГСН
	KC1_STU3_DO_HMI.A3_7_9_BULB_FAULT_ON.PV		:= true; //Лампа "Неисправность" желтая
else
	KC1_STU3_DO_HMI.A3_7_11_SOUND_ALARM1_ON.PV	:= false;
	KC1_STU3_DO_HMI.A3_7_9_BULB_FAULT_ON.PV		:= false;
end_if;

if DO_sound_AS then
	KC1_STU3_DO_HMI.A3_7_12_SOUND_ALARM2_ON.PV	:= true; //Включить аварийную сигнализацию на щите САУ УПГСН
	KC1_STU3_DO_HMI.A3_7_10_BULB_PS_AS_ON.PV	:= true; //Лампа "Авария" красная
else
	KC1_STU3_DO_HMI.A3_7_12_SOUND_ALARM2_ON.PV	:= false;
	KC1_STU3_DO_HMI.A3_7_10_BULB_PS_AS_ON.PV	:= false;
end_if;

//Сброс кнопок Деблокировка и Квитирование
Deblock := BOOL#0;
Kvitir := BOOL#0;

IF Firstscan THEN Firstscan := BOOL#0; FScan := BYTE#0; END_IF;
clock_AI_cur := CLOCK();
cycleTime_AI_sys := clock_AI_cur - clock_AI_prev;]]></ST>
